‹	let d = Fst st
‹	and rv = Snd st
‹	and set = AfSet at
‹	and mem = AfMem at
‹	in	if mem ç IxDom va ± set ç IxDom va
‹		then rv (IxVal va mem) (IxVal va set)
‹		else Lub $ºât {}
∞

